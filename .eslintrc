{
  "extends": "google",
  "env": {
  "browser": true
  },
  "globals": {
    "angular": true,
    "Firebase": true,
    "Materialize": true,
    "moment": true,
    "$": true,
    "jQuery": false
  },
  "plugins": ["flow-vars"],
  "parser": "babel-eslint",
  "rules": {
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "linebreak-style": 0,
    "strict": 0
  }
}
